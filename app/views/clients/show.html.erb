<p id="notice"><%= notice %></p>

<p class="title">
  <strong>Name:</strong>
  <%= @client.name %>
</p>
<table class="table table-striped table-bordered table-hover">
<p>
  <tr><td><strong>Email:</strong></td>
  <td><%= @client.email %></td></tr>
</p>

<p>
  <tr><td><strong>Skype:</strong></td>
  <td><%= @client.skype %></td></tr>
</p>

<p>
  <tr><td><strong>Position:</strong></td>
  <td><%= @client.position_id %></td></tr>
</p>
</table>
<br/><br/>
<button type="button" class="btn btn-default btn-lg">
  <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span> <%= link_to 'Edit', edit_client_path(@client) %>
</button>

<button type="button" class="btn btn-default btn-lg">
  <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span> <%= link_to 'Back', clients_path %>
</button>
 

